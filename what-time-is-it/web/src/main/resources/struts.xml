<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<constant name="struts.enable.DynamicMethodInvocation" value="false" />
	<constant name="struts.devMode" value="true" />

	<include file="example.xml" />

	<package name="users" namespace="/users" extends="struts-default">

		<action name="getUserList" class="userListAction">
			<result>/users/index.jsp</result>
		</action>

		<action name="addUser" class="addUserAction">
			<result>/users/addUser.jsp</result>
		</action>
	</package>

	<!-- Add addition packages and configuration here. -->
</struts>
